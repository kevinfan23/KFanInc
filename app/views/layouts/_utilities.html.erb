<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<div class="notes-wrapper">
	<div class="saver-wrap" contenteditable="false">
		<div class="notes-dot" contenteditable="false"></div>
		<div class="rest" contenteditable="false"></div>
	</div>
	<div class="notes-title" contenteditable="true" onpaste="OnPaste_StripFormatting(this, event);">
		note title
	</div>
	<div class="notes-content" contenteditable="true" onpaste="OnPaste_StripFormatting(this, event);">
	Start typing...
	</div>
	<div class="notes-line"></div>
	<div class="notes-comment"></div>
	<input type="file" onchange="previewFile()" id="selectedFile" style="display: none;" />
	<div class="getImg" onclick="document.getElementById('selectedFile').click();">
		<i class="material-icons">insert_photo</i>
	</div>
</div>


<div class="gauge green">
    <p class="gauge__values">
      <span class="gauge_rating">Good</span>
    </p>
