    <path d="M207 192.4c16.2-19.5 26-44.6 26-71.9 0-10.9-1.6-21.5-4.5-31.4l-27.3 10.2c1.8 6.8 2.7 13.9 2.7 21.2 0 19.7-6.8 37.8-18.3 52.1l21.4 19.8z" class="ring_black"/>
    <path d="M102.3 190.1c-31.4-8.1-54.6-36.6-54.6-70.5 0-40.2 32.6-72.8 72.8-72.8s72.8 32.6 72.8 72.8c0 33.9-23.2 62.5-54.6 70.5l-18.3 17.7-18.1-17.7z" class="spinner" transform='rotate(75 120 120)' />
    </svg>
</div>

<div class="twitter-streaming">
<a class="twitter-timeline" data-dnt="true" href="https://twitter.com/MrPPeople/lists/newsfeed" data-widget-id="705181877586366464" data-chrome="noscrollbar noheader nofooter noborders" width="300" height="650">
</a> 
</div>

<div id="loading-page">
  <div id="phrase_box">
  <svg width="100%" height="100%">
    <defs>
      <mask id="mask" maskUnits="userSpaceOnUse" maskContentUnits="userSpaceOnUse">
        <linearGradient id="linearGradient" gradientUnits="objectBoundingBox" x2="0" y2="1">
          <stop stop-color="white" stop-opacity="0" offset="0%"/>
          <stop stop-color="white" stop-opacity="1" offset="30%"/>
          <stop stop-color="white" stop-opacity="1" offset="70%"/>
          <stop stop-color="white" stop-opacity="0" offset="100%"/>
        </linearGradient>
        <rect width="100%" height="100%" fill="url(#linearGradient)"/>
      </mask>
    </defs>
    <g width="100%" height="100%" style="mask: url(#mask);">
      <g id="phrases"></g>
    </g>
  </svg>
  </div>
</div>

<div class="weather-app">
	<div class="main">
		<p class="temperature"></p>
		<p class="location"></p>
		<div class="toggle-degree">
			<input type="checkbox" id="toggle">
			<label for="toggle"></label>
		</div>
	</div>
    <div class="climate_bg">
	    <%= image_tag("32.svg", class: "weathericon")%>
    </div>
    <div class="info_bg">
	  <%= image_tag("droplet.svg", class: "dropicon")%>
      <p class="humidity"></p>
	  <%= image_tag("wind.svg", class: "windicon")%>
      <div class="windspeed"></div>
    </div>
</div>

